        "true-int": {
          "exitCode": 0,
          "argv": ["alpha", "--int"]
        },
        "false-int": {
          "exitCode": 0,
          "argv": ["beta", "--int"]
        }
      }
    },
    {
      "profiles": [
        {
          "path": "test/fixtures/pig-latin.ts",
          "profile": "ts"
        }
      ],
      "tests": {
        "spread": {
          "exitCode": 0,
          "argv": ["hello", "world"]
        }
      }
    },
    {
      "profiles": [
        {
          "path": "test/fixtures/invalid-positional.ts",
