{
  "profiles": {
    "ts": {
      "executables": ["c8node", "deno", "node-ts"],
      "runtimes": ["default", "deno", "tsx", "node-ts"]
    },
    "cjs": {
      "executables": ["deno", "tsx", "node-build-cjs"],
      "runtimes": ["default", "deno", "tsx", "node"]
    },
    "mjs": {
      "executables": ["deno", "tsx", "node-build-esm"],
      "runtimes": ["default", "deno", "tsx", "node"]
    }
  },
  "executables": {
    "c8tsx": "npx -s c8 --reporter=none --clean=false tsx ./src/bin/knevee.ts",
    "c8node": "npx -s c8 --reporter=none --clean=false node --experimental-strip-types --experimental-detect-module --disable-warning=MODULE_TYPELESS_PACKAGE_JSON --disable-warning=ExperimentalWarning  ./src/bin/knevee.ts",
    "deno": "deno run -A ./src/bin/knevee.ts",
    "tsx": "tsx ./src/bin/knevee.ts",
    "node-ts": "node --experimental-strip-types --experimental-detect-module --disable-warning=MODULE_TYPELESS_PACKAGE_JSON --disable-warning=ExperimentalWarning ./src/bin/knevee.ts",
    "node-build-esm": "node ./dist/bin/knevee.js",
    "node-build-cjs": "node ./dist/bin/knevee.cjs"
  },
  "runtimes": {
    "default": "",
    "deno": "deno eval --",
    "tsx": "tsx -e --",
    "node-ts": "node --experimental-strip-types --experimental-detect-module --disable-warning=MODULE_TYPELESS_PACKAGE_JSON --disable-warning=ExperimentalWarning -e --",
    "node": "node -e --"
  },
  "fixtures": [
    {
      "profiles": [
        {
          "path": "test/fixtures/greeting-passthrough.ts",
          "profile": "ts"
        },
        {
          "path": "test/fixtures/greeting-passthrough.mjs",
          "profile": "mjs"
        },
        {
          "path": "test/fixtures/greeting-passthrough.cjs",
          "profile": "cjs"
        },
        {
          "path": "test/fixtures/greeting-export.ts",
          "profile": "ts"
        },
        {
          "path": "test/fixtures/greeting-export.mjs",
          "profile": "mjs"
        },
        {
          "path": "test/fixtures/greeting-export.cjs",
          "profile": "cjs"
        }
      ],
      "tests": {
        "help": {
          "exitCode": 0,
          "argv": ["--help"]
        },
        "missing-args": {
          "exitCode": 1,
          "argv": []
        },
        "valid-args": {
          "exitCode": 0,
          "argv": ["John"]
        },
        "extra-args": {
          "exitCode": 1,
          "argv": ["John", "extra"]
        }
      }
    },
    {
      "profiles": [
        {
          "path": "test/fixtures/optional.ts",
          "profile": "ts"
        }
      ],
      "tests": {
        "help": {
          "exitCode": 0,
          "argv": ["--help"]
        },
        "missing-args": {
          "exitCode": 1,
          "argv": []
        },
        "valid-args": {
          "exitCode": 0,
          "argv": ["John"]
        },
        "extra-args": {
          "exitCode": 1,
          "argv": ["John", "Greetings", "extra"]
        }
      }
    },
    {
      "profiles": [
        {
          "path": "test/fixtures/pipe.ts",
          "profile": "ts"
        }
      ],
      "tests": {
        "stdin": {
          "pipe": "echo \"Hello World\"",
          "exitCode": 0
        }
      }
    },
    {
      "profiles": [
        {
          "path": "test/fixtures/starts-with-a.ts",
          "profile": "ts"
        }
      ],
      "tests": {
        "true": {
          "exitCode": 0,
          "argv": ["alpha"]
        },
        "false": {
          "exitCode": 0,
          "argv": ["beta"]
