{
  "profiles": {
    "ts": {
      "executables": ["c8", "deno", "node-ts"],
      "runtimes": ["default", "deno", "tsx", "node-ts"]
    },
    "cjs": {
      "executables": ["deno", "tsx", "node-build-cjs"],
      "runtimes": ["default", "deno", "tsx", "node"]
    },
    "mjs": {
      "executables": ["deno", "tsx", "node-build-esm"],
      "runtimes": ["default", "deno", "tsx", "node"]
    }
  },
  "executables": {
    "c8": "npx -s c8 --reporter=none --clean=false tsx ./src/bin/knevee.ts",
    "deno": "deno run -A ./src/bin/knevee.ts",
    "tsx": "tsx ./src/bin/knevee.ts",
    "node-ts": "node --experimental-strip-types --experimental-detect-module --disable-warning=MODULE_TYPELESS_PACKAGE_JSON --disable-warning=ExperimentalWarning ./src/bin/knevee.ts",
    "node-build-esm": "node ./dist/bin/knevee.js",
    "node-build-cjs": "node ./dist/bin/knevee.cjs"
  },
  "runtimes": {
    "default": "",
    "deno": "deno eval --",
    "tsx": "tsx -e --",
    "node-ts": "node --experimental-strip-types --experimental-detect-module --disable-warning=MODULE_TYPELESS_PACKAGE_JSON --disable-warning=ExperimentalWarning -e --",
    "node": "node -e --"
  },
  "fixtures": [
    {
      "profiles": [
        {
          "path": "test/fixtures/greeting-passthrough.ts",
          "profile": "ts"
        },
        {
          "path": "test/fixtures/greeting-passthrough.mjs",
          "profile": "mjs"
        },
        {
          "path": "test/fixtures/greeting-passthrough.cjs",
          "profile": "cjs"
        },
        {
          "path": "test/fixtures/greeting-export.ts",
          "profile": "ts"
        },
        {
          "path": "test/fixtures/greeting-export.mjs",
          "profile": "mjs"
        },
        {
          "path": "test/fixtures/greeting-export.cjs",
          "profile": "cjs"
        }
      ],
      "tests": {
        "help": [0, "--help"],
        "missing-args": [1],
        "valid-args": [0, "John"]
      }
    }
  ]
}
